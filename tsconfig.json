{
  "compilerOptions": {
    // "noImplicitAny": true,
    // "noImplicitThis": true,
    // "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@top-layer-layout/*": [
        "top-layer-layout/*"
      ]
    },
    "target": "es6",
    "lib": [
      "dom",
      "es6",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "lib/*",
    "top-layer-layout/*"
  ],
  "exclude": [
    "node_modules"
  ]
}